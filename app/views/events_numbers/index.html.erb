<h1>Listing events_numbers</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Number</th>
  </tr>

<% @events_numbers.each do |events_number| %>
  <tr>
    <td><%=h events_number.event_id %></td>
    <td><%=h events_number.number_id %></td>
    <td><%= link_to 'Show', events_number %></td>
    <td><%= link_to 'Edit', edit_events_number_path(events_number) %></td>
    <td><%= link_to 'Destroy', events_number, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New events_number', new_events_number_path %>