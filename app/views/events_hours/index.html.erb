<h1>Listing events_hours</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Hour</th>
  </tr>

<% @events_hours.each do |events_hour| %>
  <tr>
    <td><%=h events_hour.event_id %></td>
    <td><%=h events_hour.hour_id %></td>
    <td><%= link_to 'Show', events_hour %></td>
    <td><%= link_to 'Edit', edit_events_hour_path(events_hour) %></td>
    <td><%= link_to 'Destroy', events_hour, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New events_hour', new_events_hour_path %>