<h1>Listing places_hours</h1>

<table>
  <tr>
    <th>Place</th>
    <th>Hour</th>
  </tr>

<% @places_hours.each do |places_hour| %>
  <tr>
    <td><%=h places_hour.place_id %></td>
    <td><%=h places_hour.hour_id %></td>
    <td><%= link_to 'Show', places_hour %></td>
    <td><%= link_to 'Edit', edit_places_hour_path(places_hour) %></td>
    <td><%= link_to 'Destroy', places_hour, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New places_hour', new_places_hour_path %>