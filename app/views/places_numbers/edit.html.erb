<h1>Editing places_number</h1>

<% form_for(@places_number) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :place_id %><br />
    <%= f.text_field :place_id %>
  </p>
  <p>
    <%= f.label :number_id %><br />
    <%= f.text_field :number_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @places_number %> |
<%= link_to 'Back', places_numbers_path %>