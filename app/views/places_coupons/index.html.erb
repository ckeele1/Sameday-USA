<h1>Listing places_coupons</h1>

<table>
  <tr>
    <th>Place</th>
    <th>Coupon</th>
  </tr>

<% @places_coupons.each do |places_coupon| %>
  <tr>
    <td><%=h places_coupon.place_id %></td>
    <td><%=h places_coupon.coupon_id %></td>
    <td><%= link_to 'Show', places_coupon %></td>
    <td><%= link_to 'Edit', edit_places_coupon_path(places_coupon) %></td>
    <td><%= link_to 'Destroy', places_coupon, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New places_coupon', new_places_coupon_path %>